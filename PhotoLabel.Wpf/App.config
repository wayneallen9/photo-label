<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
  </configSections>
  <appSettings>
    <add key="WhereUrl" value="https://www.google.com/maps/place/{0},{1}"/>
  </appSettings>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets async="true">
      <target name="file" xsi:type="File" archiveAboveSize="5242880" layout="${longdate}|${threadid}|${callsite:includeSourcePath=False:cleanNamesOfAsyncContinuations=True:skipFrames=1}|${level}|${message}" fileName="${specialfolder:folder=LocalApplicationData}/Photo Label/log.txt" maxArchiveFiles="3"/>
    </targets>
    <rules>
      <logger name="*" minLevel="Trace" writeTo="file"/>
    </rules>
  </nlog>
  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
  </startup>
  <system.diagnostics>
    <switches>
      <add name="XmlSerialization.Compilation" value="1" />
    </switches>
  </system.diagnostics>
  <system.xml.serialization>
    <xmlSerializer  useLegacySerializerGeneration="true"/>
  </system.xml.serialization>
</configuration>
